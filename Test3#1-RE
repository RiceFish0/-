#include<stdio.h>
#include<string.h>

int main(){
    char str1[100000],str2[100000];
    int k;
    while(scanf("%s %s %d", str1, str2, &k) != EOF){
        int i = 0, j = 0;
        int len_s = strlen(str1), len_t = strlen(str2);
        while(i < len_s && j < len_t){
            if(str1[i] == str2[j]){
                i++;
            }
            j++;
        }
        
        if(i == len_s && len_t - i<=k){
            printf("Yes\n");
        }else{
            printf("No\n");
        }
    }
    return 0;
}
